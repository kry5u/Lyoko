@model IEnumerable<Lyoko.Models.Replay>



@for (int i = 0; i < Model.Count(); i++)
{
    var replay = Model.ElementAt(i);
    <tr onclick="redirectToReplay('@Url.Action("Replay", "Game", new { replayId = replay.Id })')">
        <td>></td>
        <td>@(i + 1)</td>
        <td>@replay.Score</td>
        <td>@replay.User.UserName</td>
    </tr>
}